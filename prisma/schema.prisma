generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model ServerConfig {
  id        String   @id @default(cuid())
  ownerUpn  String
  name      String
  host      String
  username  String
  pwdCipher String
  pwdIv     String
  pwdTag    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([ownerUpn])
}
